{
  "api": { "secret_key": "some secret key" },
  "subscriber_content": {
    "themes": [],
    "app": {
      "key_component": "app_router",
      "enabled": true,
      "content": {
        "functions": {
          "mount": ["function.environment.subscribe"],
          "unmount": ["function.environment.unsubscribe"]
        },
        "children": [
          {
            "key_component": "preferences",
            "enabled": true,
            "content": {
              "functions": {
                "mount": [
                  "function.preferences.subscribe",
                  "function.preferences.publish_api"
                ],
                "unmount": ["function.preferences.unsubscribe"],
                "on_click": ["function.preferences.publish_storage"]
              }
            }
          },
          {
            "key_component": "page",
            "enabled": true,
            "content": {
              "key_page": "Home",
              "functions": {
                "mount": [
                  "function.testing.log.subscribe",
                  "function.navigation.page.subscribe",
                  "function.testing.api.publish"
                ],
                "unmount": [
                  "function.testing.log.unsubscribe",
                  "function.navigation.page.unsubscribe"
                ]
              },
              "children": [
                {
                  "key_component": "header",
                  "enabled": true,
                  "content": { "text": "This is a Header" }
                },
                {
                  "key_component": "banner",
                  "enabled": true,
                  "content": {
                    "children": [
                      {
                        "key_component": "page_title",
                        "enabled": true,
                        "content": { "text": "This is the Home Page" }
                      }
                    ]
                  }
                },
                {
                  "key_component": "button",
                  "enabled": true,
                  "content": {
                    "css_key": "Filled",
                    "functions": {
                      "on_click": ["function.testing.log.publish"]
                    },
                    "text": "Test"
                  }
                },
                {
                  "key_component": "button",
                  "enabled": true,
                  "content": {
                    "css_key": "Filled",
                    "functions": {
                      "on_click": ["function.navigation.page.publish_dashboard"]
                    },
                    "text": "Dashboard"
                  }
                },
                {
                  "key_component": "footer",
                  "enabled": true,
                  "content": { "text": "This is a Footer" }
                }
              ]
            }
          },
          {
            "key_component": "page",
            "enabled": true,
            "content": {
              "key_page": "Dashboard",
              "functions": {
                "mount": ["function.navigation.page.subscribe"],
                "unmount": ["function.navigation.page.unsubscribe"]
              },
              "children": [
                {
                  "key_component": "header",
                  "enabled": true,
                  "content": { "text": "This is a Header" }
                },
                {
                  "key_component": "banner",
                  "enabled": true,
                  "content": {
                    "children": [
                      {
                        "key_component": "page_title",
                        "enabled": true,
                        "content": {
                          "text": "This is the Dashboard Page"
                        }
                      }
                    ]
                  }
                },
                {
                  "key_component": "button",
                  "enabled": true,
                  "content": {
                    "css_key": "Filled",
                    "functions": {
                      "on_click": ["function.navigation.page.publish_home"]
                    },
                    "text": "Home"
                  }
                },
                {
                  "key_component": "dashboard",
                  "enabled": true,
                  "content": {
                    "functions": {
                      "mount": [
                        "function.dashboard.api.publish",
                        "function.dashboard.api.subscribe"
                      ],
                      "unmount": ["function.dashboard.api.unsubscribe"]
                    }
                  }
                },
                {
                  "key_component": "footer",
                  "enabled": true,
                  "content": { "text": "This is a Footer" }
                }
              ]
            }
          }
        ]
      }
    },
    "modals": [],
    "notices": [],
    "assets": []
  }
